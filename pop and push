#include<stdio.h>
#include<stdlib.h>
#define MAX 4

int stack_arr[MAX];
int top = -1;

void push(int data)
{
    if(isFull())
    {
        printf("Stack Overflow");
        return;
    }
    top = top + 1;
    stack_arr[top] = data;
}
int isFull()
{
    if(top == MAX - 1)
    {
        return 1;
    }
    else{
        return 0;
    }
}
int isEmpty()
{
    if(top == -1)
    {
        return 1;
    }
    else{
        return 0;
    }
}
int pop()
{
    int value;
    if(isEmpty())
    {
        printf("Stack Underflow\n");
        exit(1);
    }
    value = stack_arr[top];
    top = top - 1;
    return value;
}
void print()
{
    int i;
    if(top == -1){
        printf("Stack Underflow\n");
        return;
    }
    for(i=top;i>=0;i--)
    {
        printf("%d\t",stack_arr[i]);
    }
    printf("\n");
}
int main()
{
    int data;
    push(1);
    push(2);
    push(3);
    push(4);
    pop();
    pop();   
    print();
    return 0;
}
